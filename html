	<script src="https://code.jquery.com/jquery-3.4.1.min.js" integrity="sha256-CSXorXvZcTkaix6Yvo6HppcZGetbYMGWSFlBw8HfCJo=" crossorigin="anonymous"></script>
	<script src="https://www.google.com/recaptcha/api.js?render=6Ldh3EUrAAAAAJ3A62QdUy5nEgmnKQ8IjqFvXAZv"></script>
	<script>
		let headUrl = "https:\/\/www.vivasam.com";
		if (headUrl === 'https://localhost:8080') {
			headUrl = "http://localhost:8080"
		}
		var menuType = 'login';
		var retCnt = null;

		$(function() {
			$("#loginFrm #userId, #loginFrm #logPw").on('click', function(){
				$("div.lBox .msg").html('');
			});

			let idFlag = false;
			$('#loginFrm').validate({
				rules : {
					userId : {
						required : true,
						minlength : 4
					},
					userPwd : {
						required : true
					}
				},
				messages : {
					userId : {
						required : "아이디를 입력해 주세요.",
						minlength : "아이디를 입력해 주세요."
					},
					userPwd : {
						required : "비밀번호를 입력해 주세요."
					}
				},
				errorPlacement : function(error, element) {
					if (element.attr("name") === "userId") {
						$("div.lBox .msg").html(error.insertBefore(element));
						$("#loginFrm #userId").focus();
						idFlag = true;
					} else {
						if (idFlag) {
							$("div.lBox .msg").append("<br/>");
							$("div.lBox .msg").append(error.insertBefore(element));
							$("#loginFrm #userId").focus();
						} else {
							$("div.lBox .msg").html(error.insertBefore(element));
							$("#loginFrm #logPw").focus();
						}
					}
				}
			});
			// 로그인 버튼 클릭 이벤트
			$('a.btn_login').click(function() {
				if (!$('#loginFrm').valid()) {
					return false;
				}
//				_loginInPage();
				// 로그인 로직 타기 전 구글 reCaptcha 인증 확인
				reCapt();
			});
			var cookieUserId = $.cookie('user_id');
			if (StringUtils.isNotEmpty(cookieUserId)) {
				$('#loginFrm').find('input:text').val(cookieUserId);
				$('#loginFrm #IdSave').prop('checked', true);
			}
			$('#loginFrm').find('input:text').focus(function() {
				if ($.trim(($.cookie('user_id') != 'null'
						&& $.cookie('user_id') != 'undefined' ? $.cookie('user_id')
								: '')) == ''
						&& $(this).val() == '아이디를 입력하세요.') {
					$(this).val('');
				}
			});

			$('#loginFrm').find('input:password').focus(function() {
				$(this).val('');
			});

			$('#loginFrm').find('input:text').blur(function() {
				if ($(this).val() == '') {
					$(this).val('');
				}
			});

			$("#loginFrm #userId").keypress(function(e) {
				if (e.keyCode == 13) {
					$("#loginFrm #logPw").focus();
					return false;
				}
			});

			$("#loginFrm #logPw").keypress(function(e) {
				if (e.keyCode == 13) {
					$('#loginFrm a.btn_login').click();
					return false;
				}
			});

			showRetCntPopup()
		});

		function _loginInPage() {

			var id = $.trim($('#loginFrm').find('input:text').val());
			var pwd = $('#loginFrm').find('input:password').val();

			if ($('#loginFrm #IdSave').is(':checked')) {
				$.cookie('user_id', id, {
					expires : 30,
					path : '/'
				});
			} else {
				$.cookie('user_id', '', {
					expires : 30,
					path : '/'
				});
			}

			$('#loginFrm').find('input:text').val(id);
			$('#loginFrm').find('input:password').val(pwd);

			Ajax.execute({
				url : '/member/signInVivasam.json',
				data : JSON.stringify({
					memberId : id,
					passwd : pwd,
					goUrl : $('#goURL').val(),
					tabId : $('#tabId').val()
				}),
				contentType : 'application/json',
				method : 'post',
				dataType : 'json',
				success : function(result) {
					var code = result.response.code;
					var redirectURL = result.response.redirectURL;
					var userSchoolLevel = result.response.userSchoolLevel;
					var msg = result.response.msg;
					if (code != 'fail') {
						// 구글 로그인 통계 남김.
						GoogleAnalyticsUtils.sendLoginInfo();
						// redirectURL : 로그인 프로세스에서 부여된 리턴 페이지 (우선순위)
						// goURL : 로그인 전 화면에서 부여한 리턴 페이지
						if (redirectURL != null && redirectURL != '') {
							redirectURL = redirectURL.includes("sso") ? redirectURL : decodeURIComponent(result.response.redirectURL);
							var goURL = $('#goURL').val();
							var tabId = $('#tabId').val();

							if (redirectURL == '/main' && goURL != '') {
								// 우선순위인 redirectURL이 default 값인 "/main"인 경우 다음 순위인 goURL 값으로 리턴
								location.href = goURL;
							} else {
								if(redirectURL == '/main') {
									// goURL이 없으며 redirectURL이 default 값인 "/main"인 경우 학교급에 맞춰 메인 페이지 리턴
									if(userSchoolLevel == 'HS'){
										location.href = "//" + globals.config.siteDomainMiddleHigh + "/main.do";
									} else {
										location.href = '/main';
									}
								} else {
									/*
                                    * 2023.6.12 songth 추가 - 샘퀴즈로 로그인 시 GA4 태그 추가
                                    * */
									if (goURL.indexOf('samquiz.vivasam.com') > 0) {
										gtag('event', 'samquiz_login', {
											'member_id': id
										});
									}

									// redirectURL이 있는 경우 우선순위이기 때문에 redirectURL 값의 페이지로 리턴
									location.href = redirectURL;
								}
							}
						} else {
							location.href = '/main';
						}
					} else {
						if (code == 'sns_fail') {
							alert(msg);
						} else if (msg != null && msg != '') {
							if (result.response.retCheck == 'false') {
								if (result.response.retCheckCnt < 5) {
									if (result.response.retCheckCnt > 0) {
										$('#retLoginCnt').text(result.response.retCheckCnt);
										$("#pwd_dialog").show();
										$("#dialog1").show();
										$("#dialog2").hide();
									} else {
										$("#pwd_dialog").show();
										$("#dialog2").show();
										$("#dialog1").hide();
									}
								} else {
									$("div.lBox .msg").html(msg);
								}

							} else {
								$("div.lBox .msg").html(msg);
							}
						}
					}
				}
			});
		}

		// 중고등 PC 비밀번호 실패 값 넘어올 경우 노출
		function showRetCntPopup() {
			if (retCnt != null) {
				if (Number(retCnt) < 5) {
					if (Number(retCnt) > 0) {
						$('#retLoginCnt').text(retCnt);
						$("#pwd_dialog").show();
						$("#dialog1").show();
						$("#dialog2").hide();
					} else {
						$("#pwd_dialog").show();
						$("#dialog2").show();
						$("#dialog1").hide();
					}
				} else {
					$("div.lBox .msg").html('아이디 또는 비밀번호가 일치하지 않습니다. 다시 입력해주세요.');
				}
			}
		}

		// 구글 reCaptcha 인증 확인(reCAPTCHA v3는 이전 버전과 달리 사용자에게 별도 액션 요구하지 않고 자동으로 점수 부여하여 봇 차단)
		function reCapt() {
			grecaptcha.ready(function() {
				grecaptcha.execute('6Ldh3EUrAAAAAJ3A62QdUy5nEgmnKQ8IjqFvXAZv', {action: 'submit'}).then(function(token) {	// 공개 사이트키 발송하여 토큰 발급
					Ajax.execute({	// 발급 받은 토큰을 서버로 발송하여 인증 처리
						url: '/member/verifyRecaptcha',
						data: {
							token: token
						},
						type: 'post',
						async: true,
						dataType: 'json',
						success: function(data) {
							if (Number(data) > 0.5) {	// 0.0(봇일 가능성 높음) & 1.0(사람일 가능성 높음)
								_loginInPage();			// 기준 충족 시 기존 로그인 로직 실행
							} else {
								alert('비정상적인 로그인 시도가 감지되었습니다. 로봇이 아닌 경우 비바샘 고객센터로 문의해주세요.');
								return;
							}
						},
						error: function (xhr, status, err) {
							alert('오류가 발생했습니다.');
						}
					});
				});
			});
		}
	</script>
	<script src="/resources/plugins/jquery/jquery.cookie.js"></script>
